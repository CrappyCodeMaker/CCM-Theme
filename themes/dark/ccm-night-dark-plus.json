      "settings": {
        "foreground": "#2B916F"
      }
    },
    {
      "name": "PL/SQL dbms",
      "scope": ["support.constant.oracleplsql"],
      "settings": {
        "foreground": "#2B916F"
      }
    },
    {
      "name": "PL/SQL constants",
      "scope": ["constant.language.oracleplsql"],
      "settings": {
        "foreground": "#50A0E9",
