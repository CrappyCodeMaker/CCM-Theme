{
	"name": "CCM Theme: Github",
	"type": "dark",
	//"semanticHighlighting": true,
	"colors": {
		//Main color
		"foreground": "#CCC",
		"focusBorder": "#FFFFFF10",
		// Activity Bar
		"activityBarBadge.background": "#3DA0FF",
		"activityBar.foreground": "#EEE",
		"activityBar.inactiveForeground": "#555",
		"activityBarBadge.foreground": "#EEE",
		"activityBar.activeBorder": "#FFBB00",
		"activityBar.background": "#171717",
		"activityBar.activeBackground": "#FFFFFF00",
		"activityBar.border": "#FFFFFF00",
		// Sidebar
		"sideBar.background": "#171717",
		"sideBarSectionHeader.background": "#171717",
		"sideBarSectionHeader.border": "#FFFFFF00",
		"sideBarSectionHeader.foreground": "#777",
		"sideBarTitle.foreground": "#DDD",
		"sideBar.border": "#FFFFFF00",
		// Badge
		"badge.background": "#444",
		"badge.foreground": "#EEE",
		//Quotes
		"textBlockQuote.background": "#FFFFFF10",
		"textBlockQuote.border": "#3DA0FFCC",
		// Button
		"button.background": "#357D40",
		"button.foreground": "#FFF",
		"button.hoverBackground": "#25A72E",
		// extensionButton
		"extensionButton.prominentBackground": "#357D40",
		"extensionButton.prominentForeground": "#FFF",
		"extensionButton.prominentHoverBackground": "#25A72E",
		// Contrast
		"contrastActiveBorder": "#FFFFFF00",
		"contrastBorder": "#FFFFFF00",
		// Foreground color for description text providing additional information, for example for a label.
		"descriptionForeground": "#4f6880",
		"selection.background": "#3c6d9b93",
		// Dropdown
		"dropdown.background": "#232323",
		"dropdown.border": "#FFFFFF00",
		// Currently found item
		"editor.findMatchBackground": "#8AFF60AA",
		"editor.findMatchBorder": "#66FF00CC",
		// Other Found Items int the document
		"editor.findMatchHighlightBackground": "#5CDEFF50",
		"editor.findMatchHighlightBorder": "#5CDEFFCC",
		// ——— Editor ———
		"editor.background": "#232323",
		"editor.foreground": "#EEE",
		"editorLineNumber.foreground": "#555",
		"editorLineNumber.activeForeground": "#EEE",
		"editorCursor.foreground": "#FFBB00",
		"editorWhitespace.foreground": "#777",
		"errorForeground": "#FF3000AA",
		"editorHoverWidget.background": "#1C1C1C",
		"editorWidget.background": "#1C1C1C",
		"editorHoverWidget.border": "#333",
		"editorError.border": "#333",
		"editorWidget.border": "#333",
		"editorCodeLens.foreground": "#777",
		// picker
		"pickerGroup.border": "#FFFFFF20",
		"pickerGroup.foreground": "#3DA0FF",
		// input
		"input.background": "#333",
		"input.border": "#FFFFFF00",
		"input.foreground": "#CCC",
		"input.placeholderForeground": "#999",
		"inputOption.activeBorder": "#FFFFFF00",
		//  The selection color battle starts here.
		"editor.selectionBackground": "#3c6d9b93",
		"editor.inactiveSelectionBackground": "#e0e0e0",
		"editor.selectionHighlightBackground": "#ADD6FF26",
		"editor.wordHighlightBackground": "#ff478448",
		"editor.wordHighlightStrongBackground": "#FFFFFF0D",
		"editor.hoverHighlightBackground": "#3c6d9b63",
		"editor.lineHighlightBackground": "#1F1F1F",
		"editor.lineHighlightBorder": "#17171750",
		"editor.rangeHighlightBackground": "#1F1F1F",
		"editorLink.activeForeground": "#4f6880",
		"editorIndentGuide.activeBackground": "#FFFFFF90",
		"editorIndentGuide.background": "#EEEEEE10",
		"editorRuler.foreground": "#A599E91C",
		"editorOverviewRuler.border": "#FFFFFF00",
		"editorBracketMatch.border": "#00AEFFAA",
		"editorBracketMatch.background": "#00AEFF50",
		"editorOverviewRuler.addedForeground": "#37d78c",
		"editorOverviewRuler.deletedForeground": "#EC3A37F5",
		"editorOverviewRuler.modifiedForeground": "#ffd6ae",
		// The editorGroupHeader
		"textPreformat.foreground": "#F3C57A",
		"textCodeBlock.background": "#10101090",
		// Errors and warnings:
		"editorError.foreground": "#FF005C",
		"editorWarning.border": "#FFFFFF00",
		"editorWarning.foreground": "#FFBB00",
		// Gutter: The gutter contains the glyph margins and the line numbers:
		"editorGutter.background": "#1C1C1C",
		"editorGutter.addedBackground": "#37d78c",
		"editorGutter.commentRangeForeground": "#008300",
		"editorGutter.deletedBackground": "#FF005C",
		"editorGutter.modifiedBackground": "#00AEFFAA",
		// Diff Editor
		"diffEditor.insertedTextBorder": "#00AEFF45",
		"diffEditor.removedTextBorder": "#FF005C45",
		"diffEditor.insertedTextBackground": "#00AEFF25",
		"diffEditor.removedTextBackground": "#FF005C25",
		// Git status colors in File Explorer
		"gitDecoration.modifiedResourceForeground": "#F3C57A",
		"gitDecoration.deletedResourceForeground": "#FF3666",
		"gitDecoration.untrackedResourceForeground": "#75C570",
		"gitDecoration.ignoredResourceForeground": "#777",
		"gitDecoration.conflictingResourceForeground": "#FF7200",
		// progressBar
		"progressBar.background": "#FFBB00",
		// Editor Groups & Tabs
		"editorGroup.emptyBackground": "#171717",
		"editorGroup.border": "#4f68802d",
		"editorGroup.dropBackground": "#444",
		// The editorGroupHeader
		"editorGroupHeader.tabsBackground": "#171717",
		"editorGroupHeader.tabsBorder": "#FFFFFF00", //top border under path
		// The tabs
		"tab.activeBackground": "#1C1C1C",
		"tab.activeForeground": "#EEE",
		"tab.border": "#FFFFFF00",
		"tab.activeBorderTop": "#3C3C3C",
		"tab.activeBorder": "#FFFFFF00",
		"tab.inactiveBackground": "#171717",
		"tab.inactiveModifiedBorder": "#FF300050",
		"tab.activeModifiedBorder": "#00BBFF",
		"tab.inactiveForeground": "#777",
		"tab.unfocusedActiveForeground": "#777",
		"tab.unfocusedInactiveForeground": "#444",
		// Menu
		"menu.foreground": "#DDD",
		"menu.separatorBackground": "#AAA",
		"menu.background": "#1C1C1C",
		"menu.selectionBackground": "#FFFFFF25",
		"menu.selectionForeground": "#FFF",
		"menubar.selectionBackground": "#FFFFFF10",
		// Lists and Trees
		"list.highlightForeground": "#0F80DD",
		"list.dropBackground": "#1C1C1C",
		"list.activeSelectionBackground": "#0D90FF77",
		"list.activeSelectionForeground": "#EEE",
		"list.focusBackground": "#0D90FF77",
		"list.focusForeground": "#FFF",
		"list.inactiveSelectionBackground": "#1C1C1C",
		"list.inactiveSelectionForeground": "#EEE",
		"tree.indentGuidesStroke": "#555",
		"list.hoverBackground": "#232323",
		"list.hoverForeground": "#FFF",
		// Tab stops
		"editor.snippetTabstopHighlightBackground": "#4f6880",
		"editor.snippetTabstopHighlightBorder": "#4f6880",
		"editor.snippetFinalTabstopHighlightBackground": "#4f6880",
		"editor.snippetFinalTabstopHighlightBorder": "#4f6880",
		// Debug menu
		"debugIcon.stopForeground": "#EC3A37F5",
		"debugIcon.continueForeground": "#FAD000",
		"debugIcon.pauseForeground": "#FAD000",
		"debugIcon.stepOverForeground": "#FAD000",
		"debugIcon.stepIntoForeground": "#FAD000",
		"debugIcon.stepOutForeground": "#FAD000",
		"debugIcon.startForeground": "#37d78c",
		"debugIcon.restartForeground": "#37d78c",
		// Scroll Bar Control
		"scrollbar.shadow": "#0C0C0C",
		"scrollbarSlider.activeBackground": "#FFFFFF50",
		"scrollbarSlider.background": "#FFFFFF20",
		"scrollbarSlider.hoverBackground": "#FFFFFF30",
		// Panel
		"panel.border": "#FFFFFF00", //bottom line above terminal
		"panel.background": "#1C1C1C",
		"panelTitle.inactiveForeground": "#555",
		"panelTitle.activeForeground": "#DDD",
		// Status Bar Colors
		// The Status Bar is shown in the bottom of the workbench
		"statusBar.foreground": "#DDD",
		"statusBar.background": "#111",
		"statusBar.border": "#FFFFFF00",
		"statusBar.noFolderBackground": "#0075BB",
		"statusBar.noFolderForeground": "#EEEEEE",
		"statusBar.noFolderBorder": "#FFFFFF00",
		"statusBarItem.remoteForeground": "#EEE",
		"statusBarItem.remoteBackground": "#16825D",
		"statusBarItem.hoverBackground": "#FFFFFF30",
		"statusBarItem.activeBackground": "#FFFFFF40",
		// notification colors - The colors below only apply for VS Code versions 1.21 and higher.
		"notificationCenter.border": "#FFFFFF00",
		"notificationCenterHeader.foreground": "#CCC",
		"notificationCenterHeader.background": "#2F2F2F",
		"notificationToast.border": "#FFFFFF00",
		"notifications.foreground": "#CCC",
		"notifications.background": "#232323",
		"notifications.border": "#FFFFFF00",
		"notificationLink.foreground": "#3DA0FF",
		// Title bar
		"titleBar.border": "#FFFFFF00",
		"titleBar.activeBackground": "#171717",
		"titleBar.activeForeground": "#777",
		"titleBar.inactiveBackground": "#232323",
		"titleBar.inactiveForeground": "#333",
		"widget.shadow": "#0C0C0C",
		"textLink.activeForeground": "#8C00FF",
		"textLink.foreground": "#3DA0FF",
		// Breadcrumbs
		"breadcrumb.background": "#1C1C1C",
		"breadcrumb.foreground": "#555",
		// Panel
		"panelTitle.activeBorder": "#3DA0FF",
		// Terminal
		"terminal.background": "#1C1C1C",
		"terminalCursor.foreground": "#FFCC00AA",
		"terminal.foreground": "#EEE",
		"terminalCursor.background": "#FFBB00",
		"terminal.ansiBlack": "#000",
		"terminal.ansiRed": "#FF3060",
		"terminal.ansiGreen": "#30DD00",
		"terminal.ansiYellow": "#FFE600",
		"terminal.ansiBlue": "#0088FF",
		"terminal.ansiMagenta": "#E60FE6",
		"terminal.ansiCyan": "#00FFFF",
		"terminal.ansiWhite": "#FFF",
		"terminal.ansiBrightBlack": "#003E80",
		"terminal.ansiBrightRed": "#FF628C",
		"terminal.ansiBrightGreen": "#8AFF60",
		"terminal.ansiBrightYellow": "#FFDD5B",
		"terminal.ansiBrightBlue": "#3DA5FF",
		"terminal.ansiBrightMagenta": "#FB94FF",
		"terminal.ansiBrightCyan": "#80FCFF",
		"terminal.ansiBrightWhite": "#FFF"
	},
	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.comment"
			],
			"settings": {
				"foreground": "#768390"
			}
		},
		{
			"scope": [
				"constant",
				"entity.name.constant",
				"variable.other.constant",
				"variable.language",
				"entity"
			],
			"settings": {
				"foreground": "#6cb6ff"
			}
		},
		{
			"scope": [
				"entity.name",
				"meta.export.default",
				"meta.definition.variable"
			],
			"settings": {
				"foreground": "#f69d50"
			}
		},
		{
			"scope": [
				"variable.parameter.function",
				"meta.jsx.children",
				"meta.block",
				"meta.tag.attributes",
				"entity.name.constant",
				"meta.object.member",
				"meta.embedded.expression"
			],
			"settings": {
				"foreground": "#adbac7"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#dcbdfb"
			}
		},
		{
			"scope": [
				"entity.name.tag",
				"support.class.component"
			],
			"settings": {
				"foreground": "#8ddb8c"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#f47067"
			}
		},
		{
			"scope": [
				"storage",
				"storage.type"
			],
			"settings": {
				"foreground": "#f47067"
			}
		},
		{
			"scope": [
				"storage.modifier.package",
				"storage.modifier.import",
				"storage.type.java"
			],
			"settings": {
				"foreground": "#adbac7"
			}
		},
		{
			"scope": [
				"string",
				"punctuation.definition.string",
				"string punctuation.section.embedded source"
			],
			"settings": {
				"foreground": "#96d0ff"
			}
		},
		{
			"scope": "support",
			"settings": {
				"foreground": "#6cb6ff"
			}
		},
		{
			"scope": "meta.property-name",
			"settings": {
				"foreground": "#6cb6ff"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#f69d50"
			}
		},
		{
			"scope": "variable.other",
			"settings": {
				"foreground": "#adbac7"
			}
		},
		{
			"scope": "invalid.broken",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ff938a"
			}
		},
		{
			"scope": "invalid.deprecated",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ff938a"
			}
		},
		{
			"scope": "invalid.illegal",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ff938a"
			}
		},
		{
			"scope": "invalid.unimplemented",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ff938a"
			}
		},
		{
			"scope": "carriage-return",
			"settings": {
				"fontStyle": "italic underline",
				"background": "#f47067",
				"foreground": "#22272e",
				"content": "^M"
			}
		},
		{
			"scope": "message.error",
			"settings": {
				"foreground": "#ff938a"
			}
		},
		{
			"scope": "string source",
			"settings": {
				"foreground": "#adbac7"
			}
		},
		{
			"scope": "string variable",
			"settings": {
				"foreground": "#6cb6ff"
			}
		},
		{
			"scope": [
				"source.regexp",
				"string.regexp"
			],
			"settings": {
				"foreground": "#96d0ff"
			}
		},
		{
			"scope": [
				"string.regexp.character-class",
				"string.regexp constant.character.escape",
				"string.regexp source.ruby.embedded",
				"string.regexp string.regexp.arbitrary-repitition"
			],
			"settings": {
				"foreground": "#96d0ff"
			}
		},
		{
			"scope": "string.regexp constant.character.escape",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#8ddb8c"
			}
		},
		{
			"scope": "support.constant",
			"settings": {
				"foreground": "#6cb6ff"
			}
		},
		{
			"scope": "support.variable",
			"settings": {
				"foreground": "#6cb6ff"
			}
		},
		{
			"scope": "meta.module-reference",
			"settings": {
				"foreground": "#6cb6ff"
			}
		},
		{
			"scope": "punctuation.definition.list.begin.markdown",
			"settings": {
				"foreground": "#f69d50"
			}
		},
		{
			"scope": [
				"markup.heading",
				"markup.heading entity.name"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#6cb6ff"
			}
		},
		{
			"scope": "markup.quote",
			"settings": {
				"foreground": "#8ddb8c"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#adbac7"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#adbac7"
			}
		},
		{
			"scope": "markup.raw",
			"settings": {
				"foreground": "#6cb6ff"
			}
		},
		{
			"scope": [
				"markup.deleted",
				"meta.diff.header.from-file",
				"punctuation.definition.deleted"
			],
			"settings": {
				"background": "#78191b",
				"foreground": "#ff938a"
			}
		},
		{
			"scope": [
				"markup.inserted",
				"meta.diff.header.to-file",
				"punctuation.definition.inserted"
			],
			"settings": {
				"background": "#113417",
				"foreground": "#8ddb8c"
			}
		},
		{
			"scope": [
				"markup.changed",
				"punctuation.definition.changed"
			],
			"settings": {
				"background": "#682d0f",
				"foreground": "#f69d50"
			}
		},
		{
			"scope": [
				"markup.ignored",
				"markup.untracked"
			],
			"settings": {
				"foreground": "#2d333b",
				"background": "#6cb6ff"
			}
		},
		{
			"scope": "meta.diff.range",
			"settings": {
				"foreground": "#dcbdfb",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#6cb6ff"
			}
		},
		{
			"scope": "meta.separator",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#6cb6ff"
			}
		},
		{
			"scope": "meta.output",
			"settings": {
				"foreground": "#6cb6ff"
			}
		},
		{
			"scope": [
				"brackethighlighter.tag",
				"brackethighlighter.curly",
				"brackethighlighter.round",
				"brackethighlighter.square",
				"brackethighlighter.angle",
				"brackethighlighter.quote"
			],
			"settings": {
				"foreground": "#768390"
			}
		},
		{
			"scope": "brackethighlighter.unmatched",
			"settings": {
				"foreground": "#ff938a"
			}
		},
		{
			"scope": [
				"constant.other.reference.link",
				"string.other.link"
			],
			"settings": {
				"foreground": "#96d0ff",
				"fontStyle": "underline"
			}
		}
	]
}