      "settings": {
        "foreground": "#838083"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "fontStyle": " italic",
        "foreground": "#838083"
      }
    },
    {
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#838083"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#838083"
      }
    },
    {
      "scope": "keyword",
