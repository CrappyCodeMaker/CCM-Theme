{
  "$schema": "vscode://schemas/color-theme",
  "name": "Dark Default Colors",
  "colors": {
    //// HEADER >>>
    "titleBar.border": "#FFFFFF10",
    "titleBar.activeBackground": "#101113",
    "titleBar.activeForeground": "#FFFFFFAA",
    //// HEADER <<<

    //// RIGHT MENU PANEL >>>
    "sideBar.background": "#101113", //FOLDERS
    "tree.indentGuidesStroke": "#FFFFFFAA", //COLOR FOR ACTIVE LINE IN FOLDER TREE
    "activityBarBadge.background": "#007ACC",
    "list.dropBackground": "#1E1E1E",
    "button.hoverBackground": "#0061b1",
    "button.background": "#1E1E1E",
    "sideBarSectionHeader.background": "#101113", //MENU HEADERS
    "activityBar.activeBackground": "#1E1E1E", //ACTIVE MENU
    "activityBar.activeBorder": "#007ACC", //ACTIVE MENU RIGHT BORDER
    "activityBar.background": "#101113", //MENU BG
    "badge.background": "#034a64", //"UNSAVED MESSAGE"
    "sideBarTitle.foreground": "#FFFFFFAA",
    "list.inactiveSelectionBackground": "#1E1E1E",
    //// RIGHT MENU PANEL <<<

    //// EDITOR >>>
    "editor.background": "#1E1E1E", //BG
    "editor.foreground": "#FFFFFFAA", //FONT COLOR
    "editor.lineHighlightBackground": "#10101045", // ACTIVE LINE
    "editor.inactiveSelectionBackground": "#3A3D41",
    "editor.selectionHighlightBackground": "#ADD6FF26",
    "editorIndentGuide.activeBackground": "#FFFFFFAA", //ACTIVE LINE
    "editorIndentGuide.background": "#EEEEEE10", //INACTIVE LINES
    "editorGroupHeader.tabsBorder": "#FFFFFF10", //TOP EDITOR BORDER
    "panel.border": "#FFFFFF10", //BOTTOM EDITOR BORDER
    "editorCursor.foreground": "#FFBB00", // CURSOR COLOR
    "editorError.foreground": "#FF005C", // ERROR's LINE COLOR
    "editorWarning.foreground": "#FFBB00", //WARNING's LINE COLOR
    "editorGroup.dropBackground": "#FFFFFF20", // COLOR WHEN YOU MOVE WINDOW
    "editorGroupHeader.tabsBackground": "#101113", //COLOR FOR TOP TAB PANEL
    "tab.border": "#FFFFFF00", //BORDER BETWEEN TABS
    "editorGroup.border": "#FFFFFF50", // BORDER BETWEEN WINDOWS
    "terminal.background": "#101113", //COLOR TRMINAL
    "dropdown.background": "#101113", //TERMINAL's MENU
    // FOR GIT
    "editorGutter.commentRangeForeground": "#008300", //GIT COMMENT LINE ???
    "editorGutter.deletedBackground": "#FF005C", //GIT DELETED LINE
    "editorGutter.modifiedBackground": "#00AEFFAA", //GIT MODIFIED LINE
    "diffEditor.insertedTextBackground": "#00AEFF25", //GIT REDACTOR NEW LINE
    "diffEditor.removedTextBackground": "#FF005C25", //GIT REDACTOR OLD LINE
    // SCROLL
    "scrollbarSlider.activeBackground": "#FFFFFF90",
    "scrollbarSlider.background": "#FFFFFF10",
    "scrollbarSlider.hoverBackground": "#FFFFFF40",
    // TABS
    "tab.activeBackground": "#1E1E1E", //ACTIV TAB
    "tab.inactiveBackground": "#101113", //INACTIVE TABS
    "tab.inactiveModifiedBorder": "#FFAE00CC", //UNSAVED TAB IN INACTIVE WINDOW
    "panelTitle.activeBorder": "#00D9FF", //UNSAVED TAB IN ACTIVE WINDOW
    // NAVIGATION
    "breadcrumb.background": "#1E1E1E",
    "breadcrumb.foreground": "#555",
    // COLORS FOR TAB WITH NUMBERS of LINE
    "editorGutter.background": "#1E1E1E", //TAB BG WITH LINE NUMBERS
    "editorLineNumber.foreground": "#555", //NUMBERS COLOR
    // COLOR FOR SELECTED BRACKETS
    "editorBracketMatch.border": "#00AEFFAA",
    "editorBracketMatch.background": "#00AEFF50",
    //// EDITOR <<<

    //// HIDING BORDERS >>>
    "statusBar.border": "#FFFFFF00",
    "activityBar.border": "#FFFFFF00",
    "sideBar.border": "#FFFFFF00",
    "sideBarSectionHeader.border": "#FFFFFF00",
    "editorOverviewRuler.border": "#FFFFFF00",
    "dropdown.border": "#FFFFFF00",
    //// HIDING BORDERS <<<

    //// BOTTOM STATUSBAR >>>
    "statusBar.foreground": "#FFFFFFAA",
    "statusBar.background": "#050B10",
    //Status Bar no Folder
    "statusBar.noFolderBackground": "#0061b1",
    "statusBar.noFolderForeground": "#EEEEEE",
    // Menu
    "menu.background": "#1E1E1E",
    "menu.foreground": "#FFFFFFAA",
    //// BOTTOM STATUSBAR <<<

    //// UNKNOWN
    "statusBarItem.remoteForeground": "#FFF",
    "statusBarItem.remoteBackground": "#16825D"
  }
}
